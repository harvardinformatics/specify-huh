<binding>
	<include path="datamodel_binding.xml" />
	<mapping name="reportLoan" class="edu.harvard.huh.specify.reports.ReportLoan">
		<value name="nameOfContact" field="nameOfContact" usage="optional" />
		<value name="institution" field="institution" usage="optional" />
		<value name="acronym" field="acronym" usage="optional" />
		<value name="address1" field="address1" usage="optional" />
		<value name="address2" field="address2" usage="optional" />
		<value name="city" field="city" usage="optional" />
		<value name="state" field="state" usage="optional" />
		<value name="zip" field="zip" usage="optional" />
		<value name="country" field="country" usage="optional" />
		<value name="nameOfShippedBy" field="nameOfShippedBy" />
		<value name="loanNumber" field="loanNumber" usage="optional" />
		<value name="dateSent" field="dateSent" usage="optional" />
		<value name="dateDue" field="dateDue" usage="optional" />
		<value name="forUseBy" field="forUseBy" usage="optional" />
		<value name="numberOfPackages" field="numberOfPackages" usage="optional" />
		<value name="generalCollectionCount" field="generalCollectionCount"
			usage="optional" />
		<value name="nonSpecimenCount" field="nonSpecimenCount" usage="optional" />
		<value name="barcodedSpecimenCount" field="barcodedSpecimenCount"
			usage="optional" />
		<value name="totalCount" field="totalCount" usage="optional" />
		<value name="description" field="description" usage="optional" />
		<collection field="barcodedSpecimens" usage="optional"
			create-type="java.util.HashSet">
			<structure
				type="edu.harvard.huh.specify.reports.ReportLoan$BarcodedSpecimen"
				name="barcodedSpecimen" usage="optional">
				<collection field="items" create-type="java.util.HashSet">
					<structure
						type="edu.harvard.huh.specify.reports.ReportLoan$BarcodedItem"
						name="barcodedItem" usage="optional">
						<value name="identifier" field="identifier" usage="optional" />
						<value name="taxon" field="taxon" usage="optional" />
						<value name="type" field="type" usage="optional" />
						<value name="collectorName" field="collectorName" usage="optional" />
						<value name="collectorNumber" field="collectorNumber" usage="optional" />
						<value name="region" field="region" usage="optional" />
					</structure>
				</collection>
			</structure>
		</collection>
		<collection field="unbarcodedSpecimens" usage="optional"
			create-type="java.util.HashSet">
			<structure
				type="edu.harvard.huh.specify.reports.ReportLoan$UnbarcodedSpecimen"
				name="unbarcodedSpecimen" usage="optional">
				<value name="sheetCount" field="sheetCount" usage="optional" />
				<value name="taxon" field="taxon" usage="optional" />
				<value name="description" field="description" usage="optional" />
			</structure>
		</collection>
	</mapping>
</binding>