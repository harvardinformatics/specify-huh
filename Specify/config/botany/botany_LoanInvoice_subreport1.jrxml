<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="LoanInvoice_subreport1" pageWidth="782" pageHeight="555" columnWidth="782" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" scriptletClass="edu.ku.brc.specify.config.Scriptlet">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="itemnum" class="java.lang.Long" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT ifnull(f.Identifier, p.Identifier) as Barcode,
                                tx.FullName as TaxonFullName,
                                ifnull(av.Name, a.LastName) as CollectorName,
                                co.FieldNumber as CollectorNumber
FROM preparation p JOIN
     fragment f ON p.PreparationID=f.PreparationID JOIN
     collectionobject co ON f.CollectionObjectID=co.CollectionObjectID JOIN
     determination d ON (f.FragmentID=d.FragmentID ) JOIN
     taxon tx ON d.TaxonID=tx.TaxonID JOIN
     collectingevent ce ON co.CollectingEventID=ce.CollectingEventID LEFT JOIN
     collector c ON (ce.CollectingEventID=c.CollectingEventID AND c.OrderNumber=1) LEFT JOIN
     agent a ON c.AgentID=a.AgentID LEFT JOIN
     agentvariant av ON (a.AgentID=av.AgentID AND av.VarType=3)

WHERE p.PreparationID = $P!{itemnum} ORDER BY Barcode LIMIT 1]]>
	</queryString>
	<field name="Barcode" class="java.lang.String"/>
	<field name="CollectorNumber" class="java.lang.String"/>
	<field name="TaxonFullName" class="java.lang.String"/>
	<field name="CollectorName" class="java.lang.String"/>
	<variable name="lotCount" class="java.lang.Integer" incrementType="Report">
		<variableExpression><![CDATA[new Integer($V{lotCount}.intValue() + 1)]]></variableExpression>
		<initialValueExpression><![CDATA[new Integer(0)]]></initialValueExpression>
	</variable>
	<variable name="specimenCount" class="java.lang.Integer">
		<variableExpression><![CDATA[new java.lang.Integer( 1 )]]></variableExpression>
		<initialValueExpression><![CDATA[new Integer(0)]]></initialValueExpression>
	</variable>
	<variable name="prepCount" class="java.lang.Integer">
		<variableExpression><![CDATA[new Integer ( 1 )]]></variableExpression>
	</variable>
