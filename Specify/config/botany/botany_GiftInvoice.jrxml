	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT g.GiftID,
                        DATE_FORMAT(shipment.ShipmentDate, '%d %M %Y') as ShipmentDate,
                        titles.Title as ShippedToTitle,
                        shippedToAgent.FirstName as ShippedToFirstName,
                        shippedToAgent.LastName as ShippedToLastName,
                        shippedToAgent.JobTitle as ShippedToJobTitle,
                        shipment.NumberOfPackages,
                        address.Address,
                        address.Address2,
                        address.City,
                        address.State,
                        address.PostalCode,
                        address.Country,
                        g.GiftNumber,
                        if(g.PurposeOfGift='Unrestricted', null, UPPER(concat('GIFT ', if(g.PurposeOfGift='ForId', 'for determination', purposes.Title), ' FROM THE ', colls.Title))) as PurposeOfGift,
                        g.Text1 as ForUseBy,
                        colls.Title as Herbarium,
                        g.SpecialConditions,
                        gp.DescriptionOfMaterial,
                        geo.FullName as Geography,
                        gp.ItemCount,
                        gp.TypeCount,
                        gp.NonSpecimenCount,
                        (gp.ItemCount + gp.TypeCount + gp.NonSpecimenCount) as Total
FROM gift g JOIN
     giftpreparation gp ON g.GiftID=gp.GiftID JOIN
     shipment on g.GiftID=shipment.GiftID JOIN
     picklistitem colls ON g.Text2=colls.Value JOIN
     geography geo ON gp.GeographyID=geo.GeographyID JOIN
     agent shippedToAgent ON (shipment.ShippedToID=shippedToAgent.AgentID AND colls.PickListID=(SELECT PickListID FROM picklist WHERE Name='HUH Collection Code')) LEFT JOIN
     picklistitem titles ON (shippedToAgent.Title=titles.Value AND titles.PickListID=(SELECT PickListID FROM picklist WHERE Name='AgentTitles')) LEFT JOIN
     picklistitem purposes ON (g.PurposeOfGift=purposes.Value AND purposes.PickListID=(SELECT PickListID FROM picklist WHERE Name='HUH Transaction Purpose')) LEFT JOIN
     address ON shippedToAgent.AgentID=address.AgentID LEFT JOIN
     giftagent ga ON g.GiftID=ga.GiftID LEFT JOIN
     agent receiverAgent ON ga.AgentID=receiverAgent.AgentID
WHERE ga.Role='Receiver' AND
      address.IsShipping AND 
      address.IsCurrent AND 
      g.GiftID $P!{itemnum}]]>
	</queryString>
