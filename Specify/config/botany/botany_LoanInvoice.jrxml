	<queryString>
		<![CDATA[SELECT loan.LoanID,
                        DATE_FORMAT(shipment.ShipmentDate, '%d %b %Y') as ShipmentDate,
                        titles.Title as ShippedToTitle,
                        shippedToAgent.FirstName as ShippedToFirstName,
                        shippedToAgent.LastName as ShippedToLastName,
                        shippedToAgent.JobTitle as ShippedToJobTitle,
                        shipment.NumberOfPackages,
                        address.Address,
                        address.Address2,
                        address.City,
                        address.State,
                        address.PostalCode,
                        address.Country,
                        ifnull(loan.SrcGeography, loan.LoanNumber) as LoanNumber,
                        UPPER(concat('loan ', if(loan.PurposeOfLoan='Unrestricted', '', if(loan.PurposeOfLoan='ForId', 'for determination', purposes.Title)), ' from the ', colls.Title)) as PurposeOfLoan,
                        loan.Text1 as ForUseBy,
                        loan.SpecialConditions,
                        tx.FullName as HigherTaxon,
                        lp.SrcTaxonomy,
                        ifnull(lp.DescriptionOfMaterial, "") as Description,
                        lp.ItemCount,
                        lp.TypeCount,
                        lp.NonSpecimenCount,
                        ifnull((select pt.Name from preptype pt where pt.PrepTypeID=p.PrepTypeID), 'Lot') as PrepType
FROM loan JOIN
     loanpreparation lp ON loan.LoanID=lp.LoanID LEFT JOIN
     preparation p ON lp.PreparationID=p.PreparationID JOIN
     shipment on loan.LoanID=shipment.LoanID JOIN
     picklistitem colls ON (loan.Text2=colls.Value AND colls.PickListID=(SELECT PickListID FROM picklist WHERE Name='HUH Collection Code')) LEFT JOIN
     taxon tx ON lp.TaxonID=tx.TaxonID JOIN
     agent shippedToAgent ON shipment.ShippedToID=shippedToAgent.AgentID LEFT JOIN
     picklistitem titles ON (shippedToAgent.Title=titles.Value AND titles.PickListID=(SELECT PickListID FROM picklist WHERE Name='AgentTitles')) LEFT JOIN
     picklistitem purposes ON (loan.PurposeOfLoan=purposes.Value AND purposes.PickListID=(SELECT PickListID FROM picklist WHERE Name='HUH Transaction Purpose')) LEFT JOIN
     address ON shippedToAgent.AgentID=address.AgentID
WHERE address.IsShipping AND 
      address.IsCurrent AND 
      loan.LoanID $P!{itemnum} ORDER BY tx.FullName]]>
	</queryString>
